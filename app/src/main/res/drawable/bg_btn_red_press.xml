<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="1dp" />
            <gradient android:angle="90"
                android:endColor="#4d0f0f" android:startColor="#531919" />
        </shape>
    </item>
    <item android:top="1dp" android:right="2dp" android:bottom="3dp" android:left="2dp">
        <shape>
            <corners android:radius="1dp" />
            <solid android:color="#7e1616" />
        </shape>
    </item>

</layer-list>
