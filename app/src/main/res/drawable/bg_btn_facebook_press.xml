<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="1dp" />
            <gradient android:angle="90"
                android:endColor="#3c435b" android:startColor="#3c435b" />
        </shape>
    </item>
    <item android:top="1dp" android:right="2dp" android:bottom="3dp" android:left="2dp">
        <shape>
            <corners android:radius="1dp" />
            <solid android:color="#3c435b" />
        </shape>
    </item>

</layer-list>
