<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="2dp" />
            <gradient android:angle="90"
                android:endColor="#cbcbcb" android:startColor="#e8e8e8" />
        </shape>
    </item>
    <item android:top="1dp" android:right="2dp" android:bottom="2dp" android:left="2dp">
        <shape>
            <corners android:radius="2dp" />
            <solid android:color="#d1ffd3" />
        </shape>
    </item>

</layer-list>
