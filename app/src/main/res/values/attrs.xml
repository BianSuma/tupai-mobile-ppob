<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AttrsInfoRow">
        <attr name="left_icon" format="reference" />
        <attr name="var_text" format="string" />
        <attr name="right_img" format="reference" />
        <attr name="tax_amount" format="boolean" />
        <attr name="status" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AttrsButton">
        <attr name="left_img" format="reference" />
        <attr name="text" format="string" />
        <attr name="sub_text" format="string" />
        <attr name="right_arrow" format="boolean" />
        <attr name="tupai_saldo" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AttrsActionBar">
        <attr name="title" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="AttrsInput">
        <attr name="hint" format="string" />
        <attr name="right_icon" format="reference" />
        <attr name="android:inputType" />
        <attr name="android:minLines" />
        <attr name="android:maxLines" />
    </declare-styleable>

    <declare-styleable name="AttrsListfindpanel">
        <attr name="panel_mode" format="string" />
    </declare-styleable>

</resources>